{
  "pageStrategy": "html5",
  "modules": {
    "base": "antie/devices/browserdevice",
    "modifiers": [
      "antie/devices/anim/styletopleft",
      "antie/devices/mediaplayer/html5",
      "antie/devices/mediaplayer/live/none",
      "antie/devices/data/nativejson",
      "antie/devices/storage/cookie",
      "antie/devices/logging/onscreen",
      "antie/devices/logging/xhr",
      "antie/devices/logging/jstestdriver",
      "antie/devices/exit/closewindow",
      "antie/devices/parentalguidance/appdefaultpghandler"
    ]
  },
  "logging": {
    "level": "none"
  },
  "streaming": {
    "video": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "h264"
          ],
          "transferFormat": [
            "plain",
            "hls"
          ],
          "maximumBitRate": 3600,
          "maximumVideoLines": 1080
        }
      ]
    },
    "audio": {
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "aac"
          ],
          "transferFormat": [
            "plain",
            "hls"
          ]
        }
      ]
    }
  },
  "input": {
    "map": {
      "8": "BACK",
      "13": "ENTER",
      "19": "PLAY_PAUSE",
      "32": "PAUSE",
      "37": "LEFT",
      "38": "UP",
      "39": "RIGHT",
      "40": "DOWN",
      "48": "0",
      "49": "1",
      "50": "2",
      "51": "3",
      "52": "4",
      "53": "5",
      "54": "6",
      "55": "7",
      "56": "8",
      "57": "9",
      "70": "FAST_FWD",
      "73": "GREEN",
      "79": "YELLOW",
      "80": "BLUE",
      "82": "REWIND",
      "85": "RED",
      "112": "INFO",
      "113": "PLAY",
      "114": "PAUSE",
      "115": "STOP",
      "116": "PREV",
      "117": "NEXT",
      "118": "REWIND",
      "119": "PLAY_PAUSE",
      "120": "FAST_FWD",
      "121": "SUBTITLE",
      "166": "BACK",
      "188": "PREV",
      "190": "NEXT",
      "402": "PLAY_PAUSE",
      "412": "REWIND",
      "413": "STOP",
      "415": "PLAY",
      "417": "FAST_FWD"
    }
  },
  "_comment": "F1 - Info; F2 PLAY; F3 PAUSE; F4 STOP; F5 skipPrev; F6 skipNext; F7 RW; F8 Play/Pause; F9 FF; F10 Subtitles",
  "accessibility": {
    "captions": {
      "supported": [
        "application/ttaf+xml"
      ]
    }
  },
  "layouts": [
    {
      "width": 960,
      "height": 540,
      "module": "%application%/appui/layouts/540p",
      "classes": [
        "browserdevice540p"
      ]
    },
    {
      "width": 1280,
      "height": 720,
      "module": "%application%/appui/layouts/720p",
      "classes": [
        "browserdevice720p"
      ]
    },
    {
      "width": 1920,
      "height": 1080,
      "module": "%application%/appui/layouts/1080p",
      "classes": [
        "browserdevice1080p"
      ]
    }
  ],
  "networking": {
    "supportsJSONP": true
  },
  "capabilities": [],
  "statLabels": {}
}
